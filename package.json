{
  "name": "vitacard-365-app",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -k \"npm:dev:vite\" \"npm:dev:server\"",
    "dev:vite": "vite",
    "dev:server": "nodemon server/index.js",
    "build": "node -e \"try{require('./tools/generate-llms.js')}catch(e){}\" && vite build",
    "preview": "vite preview",
    "android:sync": "vite build && cap sync android",
    "android:open": "cap open android",
    "android:all": "vite build && cap sync android && cap open android",
    "clean": "node -e \"const f=require('fs');const r=p=>{try{f.rmSync(p,{recursive:true,force:true})}catch{}};r('node_modules');r('dist');r('android/app/build');\""
  },
