{
    "name": "vitacard365",
    "version": "1.0.0",
    "type": "module",
    "private": true,
    "overrides": {
        "rollup": "4.21.3",
        "@rollup/rollup-win32-x64-msvc": "4.21.3",
        "@rollup/rollup-win32-ia32-msvc": "4.21.3",
        "@rollup/rollup-win32-arm64-msvc": "4.21.3"
    },
    "scripts": {
        "ios:setup": "npm install && npm run build && npx cap sync ios && bash scripts/ios/pod-install.sh",
        "dev": "vite",
        "prebuild": "node scripts/prebuild-clean-public.cjs",
        "build": "npm run prebuild && cross-env ROLLUP_SKIP_NODEJS_NATIVE=1 ROLLUP_SKIP_NATIVE=1 ROLLUP_USE_JS=1 vite build --config vite.config.mjs",
        "build:mobile": "npm run prebuild && cross-env VITE_MOBILE=1 ROLLUP_SKIP_NODEJS_NATIVE=1 ROLLUP_SKIP_NATIVE=1 ROLLUP_USE_JS=1 vite build --config vite.config.mjs",
        "clean:android:web": "node scripts/clean-android-web.cjs",
        "copy:android:clean": "npm run clean:android:web && npx cap copy android",
        "preview": "vite preview --config vite.config.mjs",
        "postbuild": "node scripts/check-css.cjs",
        "postcopy:android": "node scripts/verify-android-assets.cjs",
        "verify:android": "node scripts/verify-android-assets.cjs",
        "seed:users": "node scripts/create-seed-users.mjs",
        "admin:set-password": "node scripts/set-password-by-email.mjs",
        "backfill:profiles": "node scripts/backfill-profiles.mjs",
        "api": "node server/index.js",
        "android:debug": "npm run build:mobile && npm run copy:android:clean && npm run verify:android && cd android && .\\gradlew.bat --no-build-cache --refresh-dependencies clean assembleDebug",
        "release:android": "npm run build && npx cap sync android && cd android && .\\gradlew.bat clean bundleRelease",
        "release:ios": "npm run build && npx cap sync ios && npx cap open ios",
        "publish:android:internal": "cd android && .\\gradlew.bat publishBundle --no-daemon",
        "publish:ios:testflight": "cd ios && bundle install && bundle exec fastlane beta",
        "clean:android": "rimraf \"android/app/build\" \"android/build\" \"android/.gradle\" \"android/capacitor-cordova-android-plugins\"",
        "clean:node": "rimraf node_modules package-lock.json",
        "rebuild:android": "npm run clean:android && npx cap sync android && cd android && .\\gradlew.bat clean assembleDebug",
        "android:debug:fresh": "npm run build:mobile && npm run clean:android:web && npx cap copy android && cd android && .\\gradlew.bat --stop && .\\gradlew.bat clean assembleDebug",
        "ios:sync": "npx cap sync ios",
        "guard:lock-ios-public": "chmod -R a-w ios/App/public || true && chflags -R uchg ios/App/public || true",
        "guard:unlock-ios-public": "chmod -R u+rwX ios/App/public || true && chflags -R nouchg ios/App/public || true",
        "ios:rebuild": "npm run guard:unlock-ios-public && npm run build && npm run ios:sync && npm run guard:lock-ios-public"
    },
    "dependencies": {
        "@capacitor-community/bluetooth-le": "6.0.0",
        "@capacitor/app": "6.0.2",
        "@capacitor/browser": "^7.0.2",
        "@capacitor/device": "6.0.2",
        "@capacitor/geolocation": "6.0.2",
        "@capacitor/haptics": "6.0.2",
        "@capacitor/keyboard": "6.0.2",
        "@capacitor/local-notifications": "6.0.0",
        "@capacitor/preferences": "6.0.2",
        "@capacitor/push-notifications": "6.0.2",
        "@capacitor/share": "6.0.2",
        "@capacitor/splash-screen": "^7.0.3",
        "@emotion/is-prop-valid": "^1.4.0",
        "@radix-ui/react-accordion": "^1.1.4",
        "@radix-ui/react-dialog": "^1.1.15",
        "@radix-ui/react-dropdown-menu": "^2.1.16",
        "@radix-ui/react-label": "^2.1.7",
        "@radix-ui/react-slider": "^1.3.6",
        "@radix-ui/react-tabs": "^1.1.13",
        "@radix-ui/react-toast": "^1.2.15",
        "@supabase/auth-helpers-react": "^0.5.0",
        "@supabase/supabase-js": "^2.57.4",
        "chart.js": "^4.5.0",
        "class-variance-authority": "^0.7.1",
        "cors": "^2.8.5",
        "date-fns": "^4.1.0",
        "dotenv": "^17.2.2",
        "express": "^5.1.0",
        "framer-motion": "^12.23.12",
        "gapi-script": "^1.2.0",
        "html2canvas": "^1.4.1",
        "jspdf": "^3.0.3",
        "jspdf-autotable": "^5.0.2",
        "lucide-react": "^0.542.0",
        "mercadopago": "^2.8.0",
        "react": "^18.3.1",
        "react-chartjs-2": "^5.3.0",
        "react-dom": "^18.3.1",
        "react-easy-crop": "^5.0.6",
        "react-fastclick": "^3.0.2",
        "react-helmet": "^6.1.0",
        "react-helmet-async": "^2.0.5",
        "react-is": "^18.3.1",
        "react-router-dom": "^7.9.5",
        "recharts": "^3.2.1",
        "sisteransi": "^1.0.0",
        "tailwind-merge": "^3.3.1",
        "untildify": "^6.0.0",
        "uuid": "^12.0.0"
    },
    "devDependencies": {
        "@capacitor/android": "^6.2.1",
        "@capacitor/assets": "^3.0.5",
        "@capacitor/cli": "^6.2.1",
        "@capacitor/core": "^6.2.1",
        "@capacitor/filesystem": "^6.0.3",
        "@capacitor/ios": "^6.2.1",
        "@types/node": "^24.6.1",
        "@types/react": "^19.2.2",
        "@types/react-dom": "^19.2.2",
        "@vitejs/plugin-react": "^4.2.1",
        "@vitejs/plugin-react-swc": "^4.2.0",
        "autoprefixer": "^10.4.21",
        "cross-env": "^7.0.3",
        "postcss": "^8.5.6",
        "rimraf": "^5.0.10",
        "tailwindcss": "^3.4.17",
        "tailwindcss-animate": "^1.0.7",
        "typescript": "^5.9.3",
        "vite": "^5.4.21"
    }
}
