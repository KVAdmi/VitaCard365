{
  "name": "VitaCard365",
  "appId": "f6c253d1",
  "androidPath": "android",
  "iosPath": "ios/App",
  "build": {
    "android": {
      "stack": "Android - 2025",
      "packageManager": "npm",
      "scripts": [
        "npm ci",
        "vite build",
        "npx cap copy android && npx cap sync android"
      ],
      "env": {},
      "secrets": {},
      "hooks": [],
      "plugins": []
    },
    "ios": {
      "stack": "macOS - 2025.06 - Apple silicon",
      "packageManager": "npm",
      "scripts": [
        "npm ci",
        "npm i --no-save capacitor-healthkit@latest",
        "vite build",
        "npx cap copy ios && npx cap sync ios",
        "node scripts/ios-set-gms-key.js",
        "cd ios/App && pod install --repo-update"
      ],
      "env": {},
      "secrets": {},
      "hooks": [],
      "plugins": []
    }
  },
  "environments": [],
  "deploy": {
    "channels": [ ]
  },
  "signingCertificates": {
    "android": [],
    "ios": []
  },
  "nativeConfigs": {
    "android": [],
    "ios": []
  }
}
